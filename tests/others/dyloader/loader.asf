!Machine 64
!Kernel k
!Param 8 3
!Param 4 2

//================
//when flag == 0
//================
LEPC R0; //get kernel image addr
MOV R2, c[0x0][0x30];  //get flag_addr into R2,3
MOV R3, c[0x0][0x34];
MOV R4, c[0x0][0x20];  //get output_addr into R4,5
MOV R5, c[0x0][0x24];
LD.E.CG R1, [R2];  //get flag into R1
MOV32I R6, -1;
ISETP.EQ P0, pt, R1, R6;
@P0 ST.E [R4], R0;
@P0 EXIT;
//================
//================


//================
//when flag != -1
//================
ISETP.EQ P0, pt, R1, RZ; //flag is zero, only first block acts
@!P0 JMP c[0x0][0x38] //not zero, then jump to container
S2R R6, SR_CTAid_X;
ISETP.EQ P0, pt, R6, RZ;
@!P0 BRA !NONZERO 

//===========
//block zero
//===========
S2R R0, SR_Tid_X;
MOV R20, c[0x0][0x28]; //gpu_loadee
MOV R21, c[0x0][0x3c]; //loadee_size
MOV R22, c[0x0][0x38]; //container_addr
//R4,5 used as write pointer
	ISCADD R4, R0, c[0x0][0x38], 2; //R4 = tid<<2 + container_addr
	MOV R5, 0x1;
//R2,3 used as read pointer
	ISCADD R2, R0, c[0x0][0x28], 2; //R2 = tid<<2 + loadee_addr
	MOV R3, c[0x0][0x2c];
//R10 will be used as loading limit
	MOV R10, c[0x0][0x28];
	IADD R10, R10, c[0x0][0x3c]; //R10 = loadee_addr + loadee_size
//R11 = row size
	MOV R11, c[0x0][0x8]; //R11 = blockDim.x
	SHL R11, R11, 2;
!Label CHECK_AND_LOAD
ISETP.LT P0, pt, R2, R10;
@P0 LD.E R9, [R2];
@P0 ST.E [R4], R9;
@P0 IADD R2, R2, R11;
@P0 IADD R4, R4, R11;
@P0 BRA !CHECK_AND_LOAD
@!P0 BRA !WRITEFLAG

!Label WRITEFLAG
MEMBAR.SYS;
BAR.RED.POPC RZ, RZ; //wait
MOV R2, c[0x0][0x30];
MOV R9, 1;
ST.E.WT [R2], R9; //change flag to 1
JMP c[0x0][0x38];

//===========
//block one and later
//===========
!Label NONZERO
LD.E.CG R1, [R2];
ISETP.EQ P0, pt, R1, 1;
@P0 JMP c[0x0][0x38];
BRA !NONZERO

